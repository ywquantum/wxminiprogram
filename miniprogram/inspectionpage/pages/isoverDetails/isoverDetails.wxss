/* inspectionpage/pages/isoverDetails/isoverDetails.wxss */
.overContainer {
  width: 94%;
  height: auto;
  padding: 10px 10px;
  margin-left: 3%;
  background-color: rgb(185, 185, 185);
  margin-top: 10px;
  box-sizing: border-box;
  border-radius: 4px;
}

.inspectionImgBoxLine {
  display: flex;
  width: 100%;
  height: auto;
  flex-flow: wrap;
  justify-content: flex-start;
  margin-bottom: 10px;
}

.inspection_img {
  width: calc(33.3333% - 8px);
  height: 90px;
  margin-bottom: 10px;
  margin-right: 12px;
}

.inspection_img:nth-child(3n) {
  margin-right: 0px;
}

.imgBox {
  width: 100%;
  height: 100%;
}

.yjs_box {
  width: 100%;
  height: auto;
  margin-bottom: 10px;
}

.problem_box {
  width: 100%;
  height: auto;
  margin-bottom: 10px;
  position: relative;
}

.redPoint {
  width: 10px;
  height: 10px;
  position: absolute;
  top: 5px;
  left: 5px;
  background-color: #ff0000;
  border-radius: 50%;
}

.container_problem {
  width: calc(100% - 20px);
  height: auto;
  margin-left: 20px;
}